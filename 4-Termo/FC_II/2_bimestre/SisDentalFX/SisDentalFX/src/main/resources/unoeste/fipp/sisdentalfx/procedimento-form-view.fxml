<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="356.0" prefWidth="794.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unoeste.fipp.sisdentalfx.ProcedimentoFormController">
   <children>
      <VBox spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <Label  text="ID" />
            <TextField fx:id="tfID" />
            <Label  text="Descrição" />
            <TextField fx:id="tfDesc" />
            <Label  text="Preço" />
            <TextField fx:id="tfPreco" />
            <Label  text="Tempo" />
            <Slider fx:id="tfTempo" />
         </children>
         <VBox.margin>
            <Insets top="18.0" />
         </VBox.margin>
      </VBox>
      <HBox alignment="CENTER" maxHeight="60.0" prefHeight="78.0" prefWidth="794.0" spacing="48.0" style="-fx-background-color: gray;">
         <children>
            <Button fx:id="btConfirmar" mnemonicParsing="false" onAction="#onConfirmar" prefHeight="25.0" prefWidth="108.0" text="Confirmar" />
            <Button fx:id="btCancelar" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onCancelar" prefHeight="25.0" prefWidth="108.0" text="Cancelar" />
         </children>
      </HBox>
   </children>
</VBox>
